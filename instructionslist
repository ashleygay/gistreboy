
LISTE DES INSTRUCTIONS

LD(nn, n)	 -> done 
LD(r1, r2)  	 -> done (HL not done)
LD(A, n)	 -> done
LD(n, A)	 -> done
LD(A, (C))  	 -> done
LD((C), A)  	 -> done
LDD(A, (HL))	 -> done
LDD((HL), A)     -> done
LDI(A, (HL))     -> done
LDI((HL), A)     -> done
LDH(n, A) 	 -> done
LDH(A, n)	 -> done
LD(n, nn)	 -> done
LD(SP, HL)	 -> done
LD(HL, SP+n)	 -> done
LDHL(SP, n)	 -> done
LD(nn, SP)	 -> done

PUSH(nn)	 -> done
POP(nn)		 -> done

ADD(A, n)	 -> done 
ADC(A, n)        -> done 
SUB(n)        	 -> done 
SBC(A, n)      	 -> done 

ADD(HL, n)	 -> not done
ADD(SP, n)	 -> not done


AND(n)        	 -> done 
OR(n)		 -> done 
XOR(n)           -> done 
CP(n)            -> done 
INC(n)           -> done 
DEC(n)           -> done 

INC(nn)		 -> not done
DEC(nn)		 -> not done

SWAP(n)		 -> done

DAA		 -> done
CPL		 -> done
CCF		 -> done
SCF		 -> done
NOP 		 -> done
HALT 		 -> not done
STOP		 -> not done
DI		 -> not done
EI		 -> not done

RLC(n)		 -> done  
RL(n)		 -> done  
RRC(n)		 -> done 
RR(n)		 -> done  

SLA(n)		 -> done  
SRA(n)		 -> done 
SRL(n)		 -> done 

BIT(b, r)	 -> not done
SET(b, r)	 -> not done
RES(b, r)	 -> not done

JP(nn) 		 -> not done
JP(cc, nn)	 -> not done
JP(HL) 		 -> not done
JR(n)		 -> not done
JR(cc, n)	 -> not done

CALL(nn)	 -> not done
CALL(cc, nn)	 -> not done

RST(n)	 	 -> not done

RET 		 -> not done
RET(cc)		 -> not done
RETI		 -> not done
